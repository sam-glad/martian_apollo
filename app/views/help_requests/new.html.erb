<h2>New Help Request</h2>

<%= form_for [@help_request] do |f| %>

  <div>
    <%= f.label :subject %>
    <%= f.text_field :subject %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.label :searched %>
    <%= f.text_field :searched %>
  </div>

  <div>
    <%= f.label :problem %>
    <%= f.text_area :problem %>
  </div>

  <%= f.submit %>

<% end %>
